{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "next/core-web-vitals",
    "plugin:unicorn/recommended"
  ],
  "settings": {
    "react": "detect"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "unicorn",
    "react-hooks",
    "jsx-a11y",
    "import",
    "jest",
    "prefer-arrow-functions",
    "security",
    "functional",
    "strict-dependencies"
  ],
  "rules": {}
}
